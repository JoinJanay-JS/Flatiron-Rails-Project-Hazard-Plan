

<%= month_calendar events: schedules do |date, schedules| %>
    <%= date %>
    <% schedules.each do |schedule| %>
        <div>
            <%= link_to schedule.name, schedule, class: "link" %>
        </div>

                <%= readable_time(schedule.start_time) %> &ndash;
              <%= readable_time(schedule.end_time) %>
            </div>



    <% end %>  
<% end %> 